                     <name type="cognomen" key="Messala">
                        <expan><abbr><supplied reason="lost">Mes</supplied>sal</abbr><ex>la</ex></expan>
                     </name>
                  </persName> et <persName>
                     <name type="cognomen" key="Pedo">Pedone</name>
                  </persName>
                  <lb n="2"/>
                  <expan><abbr>co</abbr><ex>n</ex><abbr>s</abbr><ex>ulibus</ex></expan>.
               </ab>
            </div>

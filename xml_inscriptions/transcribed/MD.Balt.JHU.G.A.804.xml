                  <lb n="7" break="no"/>τοκασιγ<hi rend="ligature">νή<supplied reason="undefined" evidence="previouseditor">τ</supplied>
                  </hi>
                  <supplied reason="undefined" evidence="previouseditor">ου σήματι γειτο</supplied>
                  <lb n="8" break="no"/>
                  <supplied reason="undefined" evidence="previouseditor">νέων.</supplied>

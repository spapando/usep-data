                  <name type="cognomen" key="Victor">Victori</name>
               </persName>; <expan><abbr>vix</abbr><ex>it</ex></expan>
               <expan><abbr>ann</abbr><ex>is</ex></expan>
               <lb n="3"/>
               <num xml:lang="lat" value="25">XXV</num>, <expan><abbr>m</abbr><ex>ensibus</ex></expan>
               <num xml:lang="lat" value="7">VII</num>. <expan><abbr>Par</abbr><ex>entes</ex></expan>
               <expan><abbr>b</abbr><ex>ene</ex></expan>
               <expan><abbr>m</abbr><ex>erenti</ex></expan>
               <expan><abbr>f</abbr><ex>ecerunt</ex></expan>.
            </ab>
            <!-- The marked up transcription goes here, surrounded by <ab> </ab> elements -->

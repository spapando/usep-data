                  <name type="cognomen" key="Maximus">
                     <supplied reason="lost">Ma</supplied>ximus</name>
               </persName> et
                <lb n="5"/>
               <gap reason="lost" quantity="8" unit="character"/>s <expan><abbr>milli</abbr><ex>tes</ex></expan>
               <note>!</note>
               <lb n="6"/>

               <num xml:lang="lat" value="4">IIII</num>,
               <lb n="6"/>
               <expan><abbr>d</abbr><ex>iebus</ex></expan>
               <num xml:lang="lat" value="20">XX</num>. <persName>
                  <name type="gentilicium" key="Vettenius">Vetteni
               <lb break="no" n="7"/>us</name>
                  <name type="cognomen" key="Aeros">Aeros</name>
               </persName>
               <expan><abbr>fil</abbr><ex>io</ex></expan>

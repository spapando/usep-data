               <lb n="4" break="no"/>us Philade<hi rend="tall">f</hi>us c<supplied reason="lost">ollegae</supplied>
               <lb n="5"/>et Lucceia Vitalis tat<supplied reason="lost">ae</supplied>
               <lb n="6"/>et Misena <expan><abbr>co</abbr><ex>n</ex><abbr><hi rend="tall">i</hi>u</abbr><ex>n</ex><abbr>x</abbr></expan> b<supplied reason="lost">ene</supplied>

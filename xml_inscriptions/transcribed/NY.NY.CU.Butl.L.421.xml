               <lb n="5"/>
                  <persName>
                     <name type="gentilicium" key="Julius"><expan><abbr>Iuli</abbr><ex>us</ex></expan></name>
                     <name type="cognomen" key="Priscus">Pris
               <lb n="6"/>

                  <name type="cognomen" key="Euhodia">Euvodia</name>
               </persName>
               <lb n="5"/>
               <expan><abbr>p</abbr><ex>atrono</ex></expan>
               <expan><abbr>b</abbr><ex>ene</ex></expan>
               <expan><abbr>m</abbr><ex>erenti</ex></expan>.
            </ab>
